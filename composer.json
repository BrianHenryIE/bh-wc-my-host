{
  "name": "brianhenryie/wc-my-host",
  "description": "Attempts to determine the server's web hosting company by running a traceroute and looking up the IPs. Requires WooCommerce.",
  "license": "GPL-2.0+-or-later",
  "authors": [
    {
      "name": "BrianHenryIE",
      "email": "BrianHenryIE@gmail.com"
    }
  ],
  "autoload": {
    "classmap": ["src/"]
  },
  "autoload-dev": {
    "psr-4": { "BrianHenryIE\\MyHost\\": "tests/" }
  },
  "repositories": [
    {
      "url": "https://github.com/WordPress/wordpress-develop",
      "type": "git"
    },
    {
      "type":"composer",
      "url":"https://wpackagist.org"
    }
  ],
  "require":
    {
      "php": ">=7.4",
      "ipinfo/ipinfo": "^2.1"
    },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "wordpress/wordpress": "^5.5",
    "wpackagist-plugin/woocommerce": "*",
    "dealerdirect/phpcodesniffer-composer-installer": "*",
    "phpcompatibility/phpcompatibility-wp": "*",
    "phpstan/extension-installer": "^1.1",
    "phpunit/phpcov": "*",
    "squizlabs/php_codesniffer": "*",
    "szepeviktor/phpstan-wordpress": "^0.7.2",
    "wp-coding-standards/wpcs": "*",
    "vlucas/phpdotenv": "^5.3"
  }
}
